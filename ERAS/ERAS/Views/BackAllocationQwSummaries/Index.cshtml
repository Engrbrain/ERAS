@model IEnumerable<ERAS.Models.EBOKOutputData.BackAllocationQwSummary>

@{
    ViewBag.Title = "Index";
}

<div class="page-wrapper">
    <!-- ============================================================== -->
    <!-- Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <div class="row page-titles">
        <div class="col-md-5 align-self-center">
            <h3 class="text-themecolor">Back Allocation A</h3>
        </div>
        <div class="col-md-7 align-self-center">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                <li class="breadcrumb-item">Back ALlocation Report</li>
                <li class="breadcrumb-item active">Actual GOR Agreggated Report</li>
            </ol>
        </div>
        <div>
            <button class="right-side-toggle waves-effect waves-light btn-inverse btn btn-circle btn-sm pull-right m-l-10"><i class="ti-settings text-white"></i></button>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- End Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Container fluid  -->
    <!-- ============================================================== -->
    <div class="container-fluid">
        <!-- ============================================================== -->
        <!-- Start Page Content -->
        <!-- ============================================================== -->
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Actual GOR Aggregated Report</h4>
                        <h6 class="card-subtitle">Export data to Copy, CSV, Excel, PDF & Print, Create New Daily Entry</h6>

                        <p><a href="@Url.Action("Create", "CFBSummaries")" class="btn-info"> &nbsp;View Complete Report&nbsp; </a> </p>
                        <div class="table-responsive m-t-40" style="overflow-x: auto;">
                            <table id="example23" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>Fault Block</th>
                                        <th>RSV</th>
                                        <th>Actual GOR</th>
                                        <th class="text-nowrap">Action</th>
                                    </tr>
                                </thead>
                                <tfoot>
                                    <tr>
                                        <th>Date</th>
                                        <th>Fault Block</th>
                                        <th>RSV</th>
                                        <th>Actual GOR</th>
                                        <th class="text-nowrap">Action</th>
                                    </tr>
                                </tfoot>
                                <tbody>


                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.IndicatorDate)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.FaultBlock)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.RSV)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Qw)
                                            </td>
                                            <td>
                                                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                                                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                                                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                                            </td>
                                        </tr>
                                    }

                            </table>
